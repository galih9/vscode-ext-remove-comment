{
  "name": "g9tstool",
  "displayName": "TsTool",
  "description": "All you need for typescript development",
  "icon": "media/icon.png",
  "publisher": "galih9",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/galih9/vscode-ext-remove-comment"
  },
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "g9tstool-sidebar-view",
          "title": "TsTool",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "g9tstool-sidebar-view": [
        {
          "type": "webview",
          "id": "g9tstool-sidebar",
          "name": "Typescript tools",
          "icon": "media/icon.svg",
          "contextualTitle": "Ctxtitle"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}